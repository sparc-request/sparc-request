- if last_change.action == 'destroy'
  %tr.skinny-black-border
    %td.left
      %strike="#{@service_request.try(:protocol).try(:id)}-#{ssr.audited_changes['ssr_id'] || 'DRAFT'}"
    %td.skinny-black-border
      %strike= service_info.name
    %td.skinny-black-border.center
      %strike= AUDIT_ACTIONS[last_change.action]
- else
  %tr.skinny-black-border
    %td.left
      =ssr.display_id
    %td.skinny-black-border= service_info.name
    %td.skinny-black-border.center= AUDIT_ACTIONS[last_change.action]