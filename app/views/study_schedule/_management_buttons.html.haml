#study_schedule_buttons{ data: { protocol_id: service_request.protocol_id, sub_service_request_id: sub_service_request.id, service_request_id: service_request.id } }
  .row
    .col-sm-4.col-md-4.text-center
      =t(:protocol)[:study_schedule][:manage_arms]
      #manage_arms
        .btn.btn-success.btn-sm.glyphicon.glyphicon-plus#add_arm_button{title: t(:protocol)[:study_schedule][:add_arm], type: "button", aria: {label: "Add Arm"}}
        .btn.btn-danger.btn-sm.glyphicon.glyphicon-remove#remove_arm_button{title: t(:protocol)[:study_schedule][:remove_arm], type: "button", aria: {label: "Remove Arm"}}
        .btn.btn-warning.btn-sm.glyphicon.glyphicon-edit#edit_arm_button{title: t(:protocol)[:study_schedule][:edit_arm], type: "button", aria: {label: "Edit Arm"}}
    .col-sm-4.col-md-4.text-center
      =t(:protocol)[:study_schedule][:manage_visits]
      #manage_visit_groups
        .btn.btn-success.btn-sm.glyphicon.glyphicon-plus#add_visit_group_button{title: t(:protocol)[:study_schedule][:add_visit_group], type: "button", aria: {label: "Add Visit Group"}}
        .btn.btn-danger.btn-sm.glyphicon.glyphicon-remove#remove_visit_group_button{title: t(:protocol)[:study_schedule][:remove_visit_group], type: "button", aria: {label: "Remove Visit Group"}}
        .btn.btn-warning.btn-sm.glyphicon.glyphicon-edit#edit_visit_group_button{title: t(:protocol)[:study_schedule][:edit_visit_group], type: "button", aria: {label: "Edit Visit Group"}}
    .col-sm-4.col-md-4.text-center
      =t(:protocol)[:study_schedule][:manage_services]
      #manage_services
        .btn.btn-success.btn-sm.glyphicon.glyphicon-plus#add_service_button{title: t(:protocol)[:study_schedule][:add_service], type: "button", aria: {label: "Add Service"}}
        .btn.btn-danger.btn-sm.glyphicon.glyphicon-remove#remove_service_button{title: t(:protocol)[:study_schedule][:remove_service], type: "button", aria: {label: "Remove Service"}}