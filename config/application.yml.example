  send_emails_to_real_users:                  "false"
  send_authorized_user_emails:                "true"
  root_url:                                   "127.0.0.1:3000"
  dashboard_link:                             "/dashboard"
  admin_mail_to:                              "sparcrequest@gmail.com"
  default_mail_to:                            "sparcrequest@gmail.com"
  listserv_mail_to:                           "hewwe@musc.edu, hutsonk@musc.edu"
  new_user_cc:                                "sparcrequest@gmail.com"
  no_reply_from:                              "no-reply@musc.edu"

  ### Generic Links & URLs ###
  about_sparc_url:                            "http://academicdepartments.musc.edu/sctr/sparc_request"
  header_link_1:                              "http://sctr.musc.edu"
  header_link_2_proper:                       "127.0.0.1:3000"
  header_link_2_dashboard:                    "127.0.0.1:3000/dashboard"
  header_link_2_catalog:                      "127.0.0.1:3000/catalog_manager"
  header_link_3:                              "http://www.musc.edu"
  navbar_links:
    sparc_request:                            ['SPARCRequest',      '127.0.0.1:3000/']
    sparc_dashboard:                          ['SPARCDashboard',    '127.0.0.1:3000/dashboard']
    sparc_fulfillment:                        ['SPARCFulfillment',  'https://cwf-d.obis.musc.edu/identities/sign_in']
    sparc_catalog:                            ['SPARCCatalog',      '127.0.0.1:3000/catalog_manager']
    sparc_report:                             ['SPARCReport',       '127.0.0.1:3000/reports']
